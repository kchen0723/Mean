    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug typescript",
            "program": "${workspaceFolder}/visualizeserver/src/index.ts",
            "cwd": "${workspaceFolder}/visualizeserver",
            "sourceMaps": true,
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch runexpress.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceFolder}\\visualizeserver",
            "program": "${workspaceFolder}\\visualizeserver\\src\\runexpress.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}